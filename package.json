  "dependencies": {
    "@electron/remote": "^2.0.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "axios": "^0.26.0",
    "boostrap": "^2.0.0",
    "cors": "^2.8.5",
